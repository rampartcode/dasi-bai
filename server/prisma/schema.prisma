generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
}

model Session {
  id       String   @id @default(cuid())
  token    String   @unique
  username String   @unique
  role     String
  loggedAt DateTime @default(now())

  @@map("sessions")
}

model LdapSetting {
  id Int @id @default(autoincrement())

  serverUrl         String
  adminDn           String
  adminPassword     String
  userSearchBase    String
  usernameAttribute String

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model IncidentResponse {
  id        String   @id @default(cuid())
  createdAt DateTime @default(now())

  mes                                    String
  data                                   DateTime @map("month_date")
  incidentes_reportados                  Int
  incidentes_resolvidos_por_intervencao  Int
  incidentes_resolvidos_por_acao_tecnica Int
  incidentes_pendentes_por_investigacao  Int
  falsos_positivos                       Int
}

model Darktrace {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now())

  time        DateTime
  username    String
  ip          String
  method      String
  status      Int
  endpoint    String
  description String

  @@map("darktrace")
}

model ImpervaWaf {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now()) @map("created_at")

  alert_start_time      DateTime
  user_name_applicative String
  user_name_db          String
  source_ip             String
  source_app            String
  url                   String
  immediate_action      String
  alert_description     String
  alert_type            String
  num_of_events         Int

  @@map("imperva_waf")
}

model ImpervaDam {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now()) @map("created_at")

  num_of_events Int

  @@map("imperva_dam")
}

model CheckpointHarmony {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now()) @map("created_at")

  receive_time DateTime
  quarantined  Boolean
  subject      String
  recipients   String
  sender       String
  server_ip    String
  client_ip    String
  links        String

  @@map("checkpoint_harmony")
}

model CheckpointFwAttack {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now()) @map("created_at")

  severity String
  blade    String
  logs     Int
  attack   String

  @@map("checkpoint_fw_attack")
}

model CheckpointFw {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now()) @map("created_at")

  blade String
  logs  Int

  @@map("checkpoint_fw")
}

model Portnox {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now()) @map("created_at")

  title             String   @db.VarChar(200)
  start_info        DateTime
  events_count      String
  time_stamp        String
  device_name       String
  device_ip         String
  device_os         String   @db.VarChar(150)
  mac_address       String   @db.VarChar(20)
  network_entity_ip String
  nv_unit           String
  nv_port           Int
  action            String
  event_name        String
  connected         Boolean
  reocurrences      Int
  vr_os             String
  status_events     String

  @@map("portnox")
}

model WindowsDefender {
  Id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now()) @map("created_at")

  severity         String
  investigated_new Boolean
  first_activity   DateTime
  alert_name       String
  detect_source    String
  last_activity    DateTime
  category         String
  status           String
  classification   String
  determination    String
  assigned         String

  @@map("windows_defender")
}

model AdAudit {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now()) @map("created_at")

  user_name               String   @db.Text
  client_ip_address       String   @db.Text
  client_host_name        String   @db.Text
  domain_controller       String   @db.Text
  logon_time              DateTime
  event_type_text         String   @db.Text
  failure_reason          String   @db.Text
  client_port             Int
  user_distinguish_name   String   @db.Text
  pre_authentication_type String   @db.Text
  failure_type            String   @db.Text
  logon_service           String   @db.Text
  monitor_id              Int

  @@map("ad_audit")
}

